        <h3>Vida Inclusiva</h3>
        <ul>
          <li><i class="fa fa-road"></i> Seminario #9, oficina 102,  Providencia, Santiago.</li>
          <li><i class="fa fa-phone"></i> +56944365545</li>
          
        </ul>
      </div>
      <div class="contact">
        <h3>Pedir Reserva</h3>
        <p style="font-size: 15px; color:dodgerblue; margin-bottom:4; ">* {{msg}}</p>
        <form method="POST" action="send-email">
          <p>
            <label>Nombre</label>
            <input type="text" name="nombre">
          </p>
          <p>
            <label>Rut</label>
            <input type="text" name="rut">
          </p>
          <p>
            <label>Correo Electronico</label>
            <input type="email" name="email">
          </p>
          <p>
            <label>Numero de contacto</label>
            <input type="text" name="numerocontacto">
          </p>
          <p>
            <label>Previsión</label>
            <select name="convenio">
              <option disabled selected value>Seleccione</option>
					<option value="Consalud">Consalud</option>
					<option value="Masvida">Mas Vida</option>
					<option value="Cruzblanca">Cruz Blanca</option>
					<option value="Colmena">Colmena</option>
            </select>
          </p>
          <p class="full">
            <label>Mensaje</label>
            <textarea name="mensaje" rows="3"></textarea>
          </p>
            <p>
               <button type="submit">Pedir Reserva</button>
			   
            </p>
			
        </form>
      </div>
    </div>
